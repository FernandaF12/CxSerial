#include <LiquidCrystal.h>

LiquidCrystal lcd(12, 11, 5, 4, 3, 2); 


void setup() {
  Serial.begin(9600);
  lcd.begin(16, 2); //
  lcd.clear();
}

void loop() {
  if (Serial.available()>0)  {
      String contrasena = Serial.readString();
      lcd.setCursor(0,0);
      
      if(contrasena.equals("Fernanda") )
      {
        lcd.print("CLAVE CORRECTA");
        //lcd.clear();
      }
      else
      {
        
        lcd.print("CLAVE INCORRECTA");
        //lcd.clear();
      }
   }
   }
   
   
   
